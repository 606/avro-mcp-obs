{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Avro.Mcp.Orchestrator": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Orchestrator": {
    "ServiceName": "orchestrator",
    "MaxConcurrentForwards": 10,
    "HealthCheckInterval": "00:05:00",
    "RequestTimeout": "00:00:30"
  }
}